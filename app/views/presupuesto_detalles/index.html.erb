<p id="notice"><%= notice %></p>
<div class="row">
<div class="container">
<div class="col-md-4">
    <h2>Lista de Presupuestos Detalles</h2>
</div>
<div class="col-md-4">
</div>
<div class="col-md-4">
    <%= link_to  new_presupuesto_detalle_path, class: "btn btn-primary" do %>
      <i class="glyphicon glyphicon-plus"></i>
    <% end %>
</div>

<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th><b><i><center>Presupuesto Detalles</center></i></b></th>
      <th><b><i><center>Cantidad</center></i></b></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @presupuesto_detalles.each do |presupuesto_detalle| %>
      <tr>
        <td><%= presupuesto_detalle.presupuesto_detalle_id %></td>
        <td><%= presupuesto_detalle.cantidad %></td>
        
        <td><%= link_to presupuesto_detalle, class:"btn btn-success"  do %>
            <i class="glyphicon glyphicon-eye-open"></i>
          <% end %>
          <%= link_to edit_presupuesto_detalle_path(presupuesto_detalle), class:" btn btn-info" do  %>
            <i class="glyphicon glyphicon-pencil"></i>
          <% end %>
          <%= link_to presupuesto_detalle, method: :delete, data: { confirm: 'Estas seguro?' }, class:'btn btn-danger' do %>
            <i class="glyphicon glyphicon-trash"></i>
          <% end %>
        </td>
        </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>
<br>



